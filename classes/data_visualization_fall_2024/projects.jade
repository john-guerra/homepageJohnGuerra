doctype html
html
  head
    title Final projects - Infovis - Northeastern University
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible" content="IE=edge")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    link(rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous")

    style.

      h1 {
        margin-top: 50px;
      }

      .logo {
        height: 90px;
        border-radius: 5px;
      }


      /* body {
         background-color: #fff5df;
      } */

      #comb {
        width: 100%;
      }

      #comb .mesh {
        fill:none;
        stroke: #white;
        stroke-width: 1.5;
      }
      #comb .projectHex{
        opacity: 0.1;
      }
      #comb .projectHex.important{
        opacity: 1.0
      }
      #comb #hexClip {
        fill:black;
      }

      #comb .projectHex .hexBorder {
        stroke: #d0aba0;
        stroke-width: 1.5;
      }

      #comb .projectHex .hexBG {
        fill: white;

      }

      #comb .important .hexBG {
        fill: #ddd;
      }


      #comb circle {
        fill:none;
        stroke:steelblue;
      }
      .card {
        margin-top: 20px;
        margin-bottom: 20px;
      }

      .card .thumb {
        max-height: 400px;
      }

      .project {
        height: 600px;
        overflow: scroll;
        margin-bottom: 30px;
      }
      .project .description {
        font-size: 90%;
      }
      img.project-thumb {
        width: 100%;
        margin-bottom: 5px;
      }
      .topic {
        margin-top: 30px;
      }
      .topic h2 {
        margin-bottom: 20px;

      }
      .project_url, .project_demo {
        margin: 5px;
      }
      .intro {
        font-size: 130%;
      }

